<h1 class="lists_title container-fluid">MYfavorites</h1>

<div class="container">
 <div class="row">
   <% if @user_id = current_user.id %>
     <% @favorites_photos.each do |photo| %>
       <div class="photo-panel col-md-4">
         <h2><%= image_tag photo.image %></h2>
         <p><%= photo.content %></p>
       </div>    
     <% end %>
   <% end %>
 </div>
</div>
<br>
<%= link_to 'POST new photo', new_photo_path, class: "btn btn-primary" %>





